mixin diagram({})
  .diagram
    <svg width="100%" height="100%" viewBox="0 0 33 33" class="diagram__svg">
      <defs>
        <linearGradient id="dc-magnificently" x1="0" x2="0" y1="0" y2="1">
          <stop offset="0%" stop-color="#FFE39C"/>
          <stop offset="100%" stop-color="#FFBA9C"/>
        </linearGradient>
        <linearGradient id="dc-good" x1="0" x2="0" y1="0" y2="1">
          <stop offset="0%" stop-color="#BC9CFF"/>
          <stop offset="100%" stop-color="#8BA4F9"/>
        </linearGradient>
        <linearGradient id="dc-passably" x1="0" x2="1" y1="0" y2="0">
          <stop offset="0%" stop-color="#66D2EA"/>
          <stop offset="100%" stop-color="#6FCF97"/>
        </linearGradient>
      </defs>
      <circle class="" cx="50%" cy="50%" r="15.91549430918954" fill="transparent" stroke="url(#dc-magnificently)" stroke-width="1" stroke-dasharray="49 51" stroke-dashoffset="74.5"></circle>
      <circle class="" cx="50%" cy="50%" r="15.91549430918954" fill="transparent" stroke="url(#dc-good)" stroke-width="1" stroke-dasharray="24 76" stroke-dashoffset="24.5"></circle>
      <circle class="" cx="50%" cy="50%" r="15.91549430918954" fill="transparent" stroke="url(#dc-passably)" stroke-width="1" stroke-dasharray="24 76" stroke-dashoffset="-0.5"></circle>
    </svg>
    p.diagram__text
      span.diagram__text-count= "260"
      span.daigram__text-letter= " голосов"
